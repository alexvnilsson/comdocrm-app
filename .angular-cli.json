{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "@comdocrm/app"
  },
  "apps": [{
    "root": "src",
    "outDir": "dist",
    "assets": [
      "assets",
      "assets/i18n/translations",
      "favicon.ico",
      "assets/conf/auth.json",
      "assets/scripts/tether.min.js",
      "assets/static/oauthconstent.html",
      "assets/static/oauthcallback.html"
    ],
    "index": "index.html",
    "main": "main.ts",
    "polyfills": "polyfills.ts",
    "test": "test.ts",
    "tsconfig": "tsconfig.app.json",
    "testTsconfig": "tsconfig.spec.json",
    "prefix": "ccrm",
    "styles": [
      "assets/styles/styles.scss",
      "../node_modules/font-awesome/css/font-awesome.min.css",
      "../node_modules/tether/dist/css/tether.min.css"
    ],
    "scripts": [
      "assets/scripts/bootstrap-datepicker/index.min.js",
      "../node_modules/auth0-js/build/auth0.min.js"
    ],
    "environmentSource": ".env/environment.ts",
    "environments": {
      "dev": ".env/environment.ts",
      "prod": ".env/environment.prod.ts"
    }
  }],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [{
      "project": "src/tsconfig.app.json"
    },
    {
      "project": "src/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "scss",
    "component": {}
  }
}
